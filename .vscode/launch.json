{
    "version": "0.2.0",
    "configurations": [{
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", 
                        "--allow-net", 
                        "--allow-read=.", 
                        "--allow-write=.", 
                        "--allow-net", 
                        "--unstable", 
                        "--inspect-brk", "-A", "${file}"],
      "port": 9229,
      "outputCapture": "std"
    }]
  }